%{
#include "hpp.hpp"
%}
%defines %union { Sym*o; }
%token <o> SYM NUM OP		/* symbol number operator	*/
%token <o> LP RP			/* ()						*/
%type <o> atom ex list
%%
REPL : | REPL ex 		{ cout << $2->dump(); } ;
atom : SYM | NUM | OP ;
ex : atom
	| LP list RP		{ $$=$2; }
;
list :					{ $$=nil; }
	| ex list			{ $$=new Cons($1,$2); }
;
%%
